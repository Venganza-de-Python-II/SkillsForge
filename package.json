{
  "name": "skillsforge",
  "version": "1.0.0",
  "description": "SkillsForge - Plataforma integral para la gestión y participación en talleres de formación profesional",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mock": "concurrently \"node scripts/mock-api-server.js\" \"next dev\"",
    "mock-api": "node scripts/mock-api-server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "echo 'No tests yet'",
    "validate": "node scripts/validate-config.js",
    "predev": "exit 0",
    "postinstall": "cd infrastructure && npm install --silent || exit 0",
    "setup": "node scripts/unified-setup.js",
    "deploy": "node scripts/unified-deploy.js",
    "deploy:dev": "node scripts/unified-deploy.js dev",
    "deploy:prod": "node scripts/unified-deploy.js prod",
    "cdk": "cd infrastructure && npm run cdk --",
    "synth": "cd infrastructure && npm run synth",
    "destroy": "cd infrastructure && npm run destroy"
  },
